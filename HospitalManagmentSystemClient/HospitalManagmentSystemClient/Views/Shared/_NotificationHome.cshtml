@model IEnumerable<Models.NotificationsModel>
@{
    ViewBag.Title = "_NotificationHome";
}

<!Doctype Html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/jquery-1.12.4.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"></script>
    <link href="~/Scripts/tableStyle.css" rel="stylesheet" />

    <script type="text/javascript">       
    </script>
</head>
<body>
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">Notificaiton ID</th>
                <th scope="col">User ID</th>
                <th scope="col">Notification</th>
                <th scope="col">Generated Date</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var n in Model)
            {
                <tr>
                    <th scope="row">@n.ID</th>
                    <td>@n.UserRoleID</td>
                    <td>@n.Notifications</td>
                    <td>@n.Created_DateTime</td>
                </tr>
            }

        </tbody>
    </table>
</body>
</html>

